{
  "name": "orbs-common-library",
  "version": "0.1.0",
  "description": "orbs-common-library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/orbs-network/common-library-typescript.git"
  },
  "author": "Orbs Team",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "rebuild": "npm run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "bs58": "^4.0.1",
    "grpc": "^1.8.4",
    "grpc-caller": "^0.4.0",
