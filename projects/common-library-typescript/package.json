    "mali": "git+https://github.com/malijs/mali.git#v0.6.0",
    "nconf": "^0.10.0",
    "orbs-interfaces": "file:../architecture",
    "protobufjs": "^6.8.4",
    "secp256k1": "^3.5.0",
    "source-map-support": "^0.5.3",
    "uncaught-exception": "^6.0.2",
    "winston": "^2.4.0",
    "winston-logzio": "^1.0.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.38",
    "@types/lodash": "^4.14.98",
    "@types/nconf": "0.0.37",
    "@types/node": "^9.4.0",
    "@types/node-forge": "^0.7.1",
    "@types/protobufjs": "^6.0.0",
    "@types/winston": "^2.3.7",
    "concurrently": "^3.5.1",
    "shelljs": "^0.8.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
